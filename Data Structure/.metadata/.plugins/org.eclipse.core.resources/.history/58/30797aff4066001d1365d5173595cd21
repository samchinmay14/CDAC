package queue_3;

public class Element 
{
	private int data;
	private int prio;
	
	public Element()
	{
		data=-1;
		prio=-1;
	}
	public Element(int d,int p)
	{
		data=d;
		prio=p;
	}
	public void display()
	{
		System.out.println("Data : "+data+"\nPriority : "+prio);
	}
	public String toString()
	{
		return "Data:"+data+"\nPriority : "+prio;
	}
}
